curl -sSL -o kubectl https://dl.k8s.io/release/v1.25.0/bin/linux/amd64/kubectl
chmod +x ./kubectl
curl -sSL -o argocd https://github.com/argoproj/argo-cd/releases/latest/download/argocd-linux-amd64
chmod +x ./argocd
echo "export PATH=$PATH:/home/jovyan" >> /home/jovyan/.bash_profile
curl -sSL -o helm.tar.gz https://get.helm.sh/helm-v3.10.0-rc.1-linux-amd64.tar.gz
tar -xvf helm.tar.gz
mv linux-amd64/helm ./helm
rm -fr linux-amd64
chmod +x ./helm
